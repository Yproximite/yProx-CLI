{
  "name": "@yproximite/yprox-cli",
  "version": "0.0.0-development",
  "description": "CLI tools for yProx CMS",
  "repository": "https://github.com/Yproximite/yProx-cli.git",
  "author": "Hugo Alliaume <kocal@live.fr>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": {
    "yprox-cli": "dist/bin/yprox-cli.js"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "build": "tsc; chmod +x ./dist/bin/yprox-cli.js",
    "watch": "tsc --watch",
    "lint": "tslint {bin,lib,types}/**/*.ts",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@kocal/logger": "^1.1.1",
    "autoprefixer": "^9.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "buble": "^0.19.6",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "cssnano": "^4.0.5",
    "defaults-deep": "^0.2.4",
    "dotenv": "^6.1.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-terser": "^1.1.6",
    "gulp-watch": "^5.0.1",
    "joi": "^14.0.4",
    "merge-stream": "^1.0.1",
    "minimist": "^1.2.0",
    "node-sass": "^4.10.0",
    "node-sass-tilde-importer": "^1.0.2",
    "rollup": "^0.68.0",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-graphql": "^0.1.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^3.0.0",
    "rollup-plugin-vue": "^4.3.2",
    "stylelint-formatter-pretty": "^1.0.3"
  },
  "peerDependencies": {
    "eslint": ">= 5.0.0",
    "stylelint": ">= 9.0.0"
  },
  "devDependencies": {
    "@kocal/semantic-release-preset": "^1.1.0",
    "@types/autoprefixer": "^9.1.1",
    "@types/cssnano": "^4.0.0",
    "@types/dotenv": "^6.1.0",
    "@types/eslint": "^4.16.5",
    "@types/gulp": "^4.0.5",
    "@types/gulp-concat": "^0.0.32",
    "@types/gulp-if": "^0.0.33",
    "@types/gulp-imagemin": "^4.1.1",
    "@types/gulp-sass": "^0.0.31",
    "@types/gulp-sourcemaps": "^0.0.32",
    "@types/gulp-watch": "^4.1.34",
    "@types/joi": "^14.0.1",
    "@types/rollup-plugin-json": "^3.0.1",
    "@types/stylelint": "^9.4.1",
    "eslint": "^5.11.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "semantic-release": "^15.10.8",
    "stylelint": "^9.9.0",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "vuepress": "^0.14.8"
  }
}
