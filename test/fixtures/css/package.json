{
  "private": true,
  "dependencies": {
    "bootstrap": "4.3.1"
  },
  "yproxCli": {
    "assets": {
      "app": [
        {
          "handler": "sass",
          "src": "src/bootstrap.scss",
          "dest": "dist/",
          "destFile": "bootstrap.css"
        },
        {
          "handler": "css",
          "src": "src/style.css",
          "dest": "dist/",
          "concat": "style.css"
        }
      ]
    }
  }
}
